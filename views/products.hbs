<div class="row">
  <div class"col-md-12">
   <!--<h1>End Point de productos</h1> -->
   <h2 class="text-center"><p class="bg-primary">{{title}}</p></h2>
 </div>
</div>

<form class="bg-success">
<div>
  <a class="btn btn-info" href="/products/new" role="button">Add New Product</a>
</div>

<div class="row">
  {{#each products}}
  <div class="col-md-4">
   <h2 class="text-center">{{this.name}}</h2>
   <img src="{{this.photo}}" class="img-responsive" alt="{{this.name}}">
   <p class="text-center">Stock: {{this.stock}}</p>
 </div>
 {{/each}}
 </div>
</form>
